import com.qualcomm.robotcore.eventloop.opmode.LinearOpMode;



//------------------------------------------------------------------------------
//
// PushBotHardware
//

/**
 * Provides a single hardware access point between custom op-modes and the
 * OpMode class for the Push Bot.
 *
 * This class prevents the custom op-mode from throwing an exception at runtime.
 * If any hardware fails to map, a warning will be shown via telemetry data,
 * calls to methods will fail, but will not cause the application to crash.
 *
 * @author SSI Robotics
 * @version 2015-08-13-20-04
 *
 * Modified by Labanya Mukhopadhyay
 * Athena FTC team, EVHS,  San Jose
 */
//public class AthenaBotManual extends OpMode
public class AthenaBotManual_1 extends OpMode

{
    //--------------------------------------------------------------------------
    //
    // PushBotHardware
    //
    final static double HAND_R_MIN_RANGE  = 0.0;
    final static double HAND_R_MAX_RANGE  = 0.7;
    final static double HAND_L_MIN_RANGE  = 0.0;
    final static double HAND_L_MAX_RANGE  = 0.7;




    /**
     * Construct the class.
     *
     * The system calls this member when the class is instantiated.
     */
    public AthenaBotManual_1()

    {
        //
        // Initialize base classes.
        //
        // All via self-construction.

        //
        // Initialize class members.
        //
        // All via self-construction.

    } // PushBotHardware

    //--------------------------------------------------------------------------
    //
    // init
    //
    /**
     * Perform any actions that are necessary when the OpMode is enabled.
     *
     * The system calls this member once when the OpMode is enabled.
     */
    @Override public void init ()

    {
        //
        // Use the hardwareMap to associate class members to hardware ports.
        //
        // Note that the names of the devices (i.e. arguments to the get method)
        // must match the names specified in the configuration file created by
        // the FTC Robot Controller (Settings-->Configure Robot).
        //
        // The variable below is used to provide telemetry data to a class user.
        //
        v_warning_generated = false;
        v_warning_message = "Can't map; ";

        //
        // Connect the drive wheel motors.
        //
        // The direction of the right motor is reversed, so joystick inputs can
        // be more generically applied.
        //
        try
        {
            v_motor_left_drive = hardwareMap.dcMotor.get ("motor_1");
            //v_motor_left_drive.setDirection (DcMotor.Direction.REVERSE);
        }
        catch (Exception p_exeception)
        {
            m_warning_message ("motor_1");
            DbgLog.msg (p_exeception.getLocalizedMessage ());

            v_motor_left_drive = null;
        }

        try
        {
            v_motor_right_drive = hardwareMap.dcMotor.get ("motor_2");
            v_motor_right_drive.setDirection (DcMotor.Direction.REVERSE);
        }
        catch (Exception p_exeception)
        {
            m_warning_message ("motor_2");
            DbgLog.msg (p_exeception.getLocalizedMessage ());

            v_motor_right_drive = null;
        }

        try {
            v_motor_left_drive_2 = hardwareMap.dcMotor.get("motor_3");
            //v_motor_right_drive.setDirection (DcMotor.Direction.REVERSE);
        }
        catch (Exception p_exeception)
        {
            m_warning_message ("motor_3");
            DbgLog.msg (p_exeception.getLocalizedMessage ());

            v_motor_left_drive_2 = null;
        }

        try
        {
            v_motor_right_drive_2 = hardwareMap.dcMotor.get ("motor_4");
            v_motor_right_drive_2.setDirection (DcMotor.Direction.REVERSE);
        }
        catch (Exception p_exeception)
        {
            m_warning_message ("motor_4");
            DbgLog.msg (p_exeception.getLocalizedMessage ());

            v_motor_right_drive_2 = null;
        }

        //
        // Connect the left arm motor.
        //
        try
        {
            v_motor_arm = hardwareMap.dcMotor.get ("motor_5");
        }
        catch (Exception p_exeception)
        {
            m_warning_message ("motor_5");
            DbgLog.msg (p_exeception.getLocalizedMessage ());

            v_motor_arm = null;
        }

        //

        // Connect the Tape measure motor.

        try
        {
            v_motor_tape_msr = hardwareMap.dcMotor.get ("motor_6");
        }
        catch (Exception p_exeception)
        {
            m_warning_message ("motor_6");
            DbgLog.msg (p_exeception.getLocalizedMessage ());

            v_motor_tape_msr = null;
        }


        //
        // Connect the servo motors.
        //
        // Indicate the initial position of both the left and right servos.  The
        // hand should be closed in the beginning.
        //
        double arm_elbow_position = 1.0;
        double arm_elbow_position2 = 0.0;


//     Servo for arm elbow
        try
        {
            v_servo_arm_elbow = hardwareMap.servo.get ("servo_1");
            v_servo_arm_elbow.setPosition (arm_elbow_position);
        }
        catch (Exception p_exeception)
        {
            m_warning_message ("servo_1");
            DbgLog.msg (p_exeception.getLocalizedMessage ());

            v_servo_arm_elbow = null;
        }


        try
        {
            v_servo_arm_elbow2 = hardwareMap.servo.get ("servo_2");
            v_servo_arm_elbow2.setPosition (arm_elbow_position2);
        }
        catch (Exception p_exeception)
        {
            m_warning_message ("servo_1");
            DbgLog.msg (p_exeception.getLocalizedMessage ());

            v_servo_arm_elbow2 = null;
        }

    //    double l_fling_position = 0.0;
        double r_fling_position = 1.0;
        // Servo for left fling
  /*      try
        {
            v_servo_left_fling = hardwareMap.servo.get ("servo_2");
            v_servo_left_fling.setPosition (l_fling_position);
        }

        catch (Exception p_exeception)
        {
            m_warning_message ("servo_2");
            DbgLog.msg (p_exeception.getLocalizedMessage ());

   //         v_servo_left_fling = null;
        }
*/
        // Servo for right fling
        try
        {
            v_servo_right_fling = hardwareMap.servo.get ("servo_3");
            v_servo_right_fling.setPosition (r_fling_position);
        }
        catch (Exception p_exeception)
        {
            m_warning_message ("servo_3");
            DbgLog.msg (p_exeception.getLocalizedMessage ());

            v_servo_right_fling = null;
        }

        double l_zipline_position = 0.0;
        double r_zipline_position = 1.0;

        // Servo for left zipline trigger
        try
        {
            v_servo_left_zipline = hardwareMap.servo.get ("servo_4");
            v_servo_left_zipline.setPosition (l_zipline_position);
        }
        catch (Exception p_exeception)
        {
            m_warning_message ("servo_4");
            DbgLog.msg (p_exeception.getLocalizedMessage ());

            v_servo_left_zipline = null;
        }

        // Servo for right zipline
        try
        {
            v_servo_right_zipline = hardwareMap.servo.get ("servo_5");
            v_servo_right_zipline.setPosition (r_zipline_position);
        }
        catch (Exception p_exeception)
        {
            m_warning_message ("servo_5");
            DbgLog.msg (p_exeception.getLocalizedMessage ());

            v_servo_right_zipline = null;
        }


    } // init

    //--------------------------------------------------------------------------
    //
    // a_warning_generated
    //
    /**
     * Access whether a warning has been generated.
     */
    boolean a_warning_generated ()

    {
        return v_warning_generated;

    } // a_warning_generated

    //--------------------------------------------------------------------------
    //
    // a_warning_message
    //
    /**
     * Access the warning message.
     */
    String a_warning_message ()

    {
        return v_warning_message;

    } // a_warning_message

    //--------------------------------------------------------------------------
    //
    // m_warning_message
    //
    /**
     * Mutate the warning message by ADDING the specified message to the current
     * message; set the warning indicator to true.
     *
     * A comma will be added before the specified message if the message isn't
     * empty.
     */
    void m_warning_message (String p_exception_message)

    {
        if (v_warning_generated)
        {
            v_warning_message += ", ";
        }
        v_warning_generated = true;
        v_warning_message += p_exception_message;

    } // m_warning_message

    //--------------------------------------------------------------------------
    //
    // start
    //
    /**
     * Perform any actions that are necessary when the OpMode is enabled.
     *
     * The system calls this member once when the OpMode is enabled.
     */
    @Override public void start ()

    {
        //
        // Only actions that are common to all Op-Modes (i.e. both automatic and
        // manual) should be implemented here.
        //
        // This method is designed to be overridden.
        //

    } // start

    //--------------------------------------------------------------------------
    //
    // loop
    //
    /**
     * Perform any actions that are necessary while the OpMode is running.
     *
     * The system calls this member repeatedly while the OpMode is running.
     */
    @Override public void loop ()

    {
        //
        // Only actions that are common to all OpModes (i.e. both auto and\
        // manual) should be implemented here.
        //
        // This method is designed to be overridden.
        //
       /*
       *
       * Gamepad 1 controls the motors via the left and right sticks
       */

        //
        // Manage the drive wheel motors.
        //
        float l_left_drive_power = scale_motor_power (-gamepad1.left_stick_y);
        float l_right_drive_power = scale_motor_power (-gamepad1.right_stick_y);

        set_drive_power (l_left_drive_power, l_right_drive_power);

       /*
       *
       * Gamepad 2 controls the Arm shoulder motors via
       * the left stick
       */

       float l_arm_power = scale_motor_power (-gamepad2.left_stick_y)/2;
        m_arm_power(l_arm_power);

       /*
       *
       * Gamepad 2's  controls the Tape Measure motors via
       * the right stick
       */

        float l_tape_msr_power = scale_motor_power (-gamepad2.right_stick_y);
        m_tape_msr_power (l_tape_msr_power);

        //----------------------------------------------------------------------
        //
        // Servo Motors
        //
        // Obtain the current values of the gamepad 'x' and 'b' buttons.
        //
        // Note that x and b buttons have boolean values of true and false.
        //
        // The clip method guarantees the value never exceeds the allowable range of
        // [0,1].
        //
        // The setPosition methods write the motor power values to the Servo
        // class, but the positions aren't applied until this method ends.

        /*
         Gamepad 2 'x' will open the left elbow, 'a' will close the left elbow

         */

        //
        // amount to change the elbow servo position by
        double l_hand_Delta = 0.2;
        double r_hand_Delta = 0.2;

        if (gamepad2.left_bumper)
        {
            m_arm_elbow_position(a_arm_elbow_position() - 0.02);
        }
        else if (gamepad2.right_bumper)
        {
            m_arm_elbow_position(a_arm_elbow_position() + 0.02);
        }


        if (gamepad2.left_bumper)
        {
            m_arm_elbow_position2(a_arm_elbow_position2() + 0.02);
        }
        else if (gamepad2.right_bumper)
        {
            m_arm_elbow_position2(a_arm_elbow_position2() - 0.02);
        }

        // Left Fling control

    /*    if (gamepad2.x)
        {
            m_left_fling_position(a_left_fling_position() + 0.02);
        }
        else if (gamepad2.a)
        {
            m_left_fling_position(a_left_fling_position() - 0.02);
        }
*/
        // Right Fling control

        if (gamepad2.y)
        {
            m_right_fling_position(a_right_fling_position() + 0.05);
        }
        else if (gamepad2.b)
        {
            m_right_fling_position(a_right_fling_position() - 0.05);
        }

        // Left Zipline control

        if (gamepad1.x)
        {
            m_left_zipline_position(a_left_zipline_position() + 0.05);
        }
        else if (gamepad1.a)
        {
            m_left_zipline_position(a_left_zipline_position() - 0.05);
        }

        // Right Zipline control

        if (gamepad1.y)
        {
            m_right_zipline_position(a_right_zipline_position() + 0.05);
        }
        else if (gamepad1.b)
        {
            m_right_zipline_position(a_right_zipline_position() - 0.05);
        }

        //
        // Send telemetry data to the driver station.
        //
        update_telemetry (); // Update common telemetry
        update_gamepad_telemetry ();

    } // loop

    //--------------------------------------------------------------------------
    //
    // stop
