    public void init_arm() {

        if (v_servo_arm_elbow != null)
        {
            v_servo_arm_elbow.setPosition(1.0);
        }
    }

    public void raise_arm() {
        if (v_servo_arm_elbow != null)
        {
            v_servo_arm_elbow.setPosition(0.5);
        }

    }

